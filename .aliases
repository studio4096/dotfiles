# Unix
alias la='ls -a'
alias ll='ls -l'

alias gs='git status'
alias ga='git add'
alias gc='git commit -m'
alias gd='git diff'
alias gp='git push'

alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias rsync='echo -n "Check \"-n\" option. OK? [y/N]: "; [[ "$(read _a; echo -n $_a)" == "y" ]]&& rsync '

alias mkdir='mkdir -p'

# sudo の後のコマンドでエイリアスを有効にする
alias sudo='sudo '
 



# Include custom aliases
[[ -f ~/.aliases.local ]] && source ~/.aliases.local
